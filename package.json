{
  "name": "conectorContaplusSmartBI",
  "productName": "Conector Contaplus smartBI",
  "description": "Conector de smartBI con Contaplus",
  "version": "1.0.1",
  "author": "smartBI by Five",
  "copyright": "© 2017, Five S.L.",
  "homepage": "https://smartbi.acens.net",
  "license": "MIT",
  "main": "app/background.js",
  "build": {
    "appId": "com.fivecorporation.smartbi.contaplus.desktop",
    "copyright": "Copyright © 2017 Five S.L.",
    "nsis": {
      "language": "3082"
    },
    "squirrelWindows": {
      "useAppIdAsId": true
    },
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "config": {
    "update": {
      "url": "https://update.smartbi.acens.net/contaplus/"
    }
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build",
    "start": "gulp start",
    "pretest": "gulp build-unit --env=test",
    "test": "electron-mocha app/specs.js.autogenerated --renderer --require source-map-support/register",
    "coverage": "npm test -- -R scripts/istanbul-reporter",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register"
  },
  "dependencies": {
    "auto-launch": "^5.0.1",
    "drivelist": "^5.0.9",
    "electron-config": "^0.2.1",
    "fs-jetpack": "^0.10.2",
    "jquery": "^1.12.4",
    "jsonwebtoken": "^7.2.1",
    "node-dbf": "^0.2.1",
    "unirest": "^0.5.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "electron": "^1.4.7",
    "electron-builder": "^12.3.1",
    "electron-mocha": "^3.0.0",
    "gulp": "^3.9.0",
    "gulp-batch": "^1.0.5",
    "gulp-less": "^3.0.3",
    "gulp-plumber": "^1.1.0",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.5",
    "istanbul": "^0.4.3",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "rollup": "^0.41.4",
    "rollup-plugin-istanbul": "^1.1.0",
    "source-map-support": "^0.4.11",
    "spectron": "^3.5.0"
  }
}
